<head>
    <script>
        function callMe() {
            alert('Clicked Me');
        };
        function callFunction(fn){
            fn();
        };

        var greet = function() {
            alert("hai , How are you?");
        }

    </script>
</head>

<body>
    <button onclick="greet()">Click Me</button>
</body>

